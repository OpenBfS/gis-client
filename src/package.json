{
    "private": false,
    "name": "@openbfs/imis3gis",
    "version": "1.18.9",
    "description": "IMIS3 GIS",
    "scripts": {
        "pretest": "npm run lint",
        "lint": "eslint app/ classic/ modern/ overrides/ && eslint test/",
        "lint-fix": "eslint --fix app/ classic/ modern/ && eslint --fix test/",
        "test": "npm-run-all --parallel --race test:all",
        "test:all": "npm-run-all test:classic test:modern copy-coverages",
        "test:classic": "karma start karma.conf.js --single-run --toolkit classic",
        "test:modern": "karma start karma.conf.js --single-run --toolkit modern",
        "test:watch": "karma start karma.conf.js",
        "copy-coverages": "cp coverage/modern/*/*json coverage/coverage1.json && cp coverage/classic/*/*json coverage/coverage2.json",
        "combine-coverages": "nyc merge coverage/ coverage-combined/coverage.json",
        "coverage-summary": "nyc report -t coverage-combined",
        "userdoc": "gitbook pdf ../docs/userdoc ../docs/userdoc/User-Documentation.pdf",
        "jsduck": "jsduck --config jsduck-config.json",
        "clean": "rimraf apidoc build/* coverage coverage-combined"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OpenBfS/gis_client.git"
    },
    "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
    "license": "GPL-3.0",
    "devDependencies": {
        "acorn": "8.15.0",
        "braces": "3.0.3",
        "eslint": "9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "expect.js": "0.3.1",
        "express": "5.2.1",
        "jsduck": "1.1.2",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-expect": "1.1.3",
        "karma-firefox-launcher": "2.1.3",
        "karma-mocha": "2.0.1",
        "karma-mocha-reporter": "2.2.5",
        "karma-sinon": "1.0.5",
        "lodash": "4.17.21",
        "minimatch": "10.1.1",
        "mocha": "11.7.5",
        "npm-run-all2": "8.0.4",
        "nyc": "17.1.0",
        "rimraf": "6.1.2",
        "serve-static": "2.2.1",
        "sinon": "21.0.1"
    },
    "bugs": {
        "url": "https://github.com/OpenBfS/gis_client/issues"
    },
    "homepage": "https://github.com/OpenBfS/gis_client#readme",
    "main": "app.js",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "echarts": "^5.4.1",
        "geostyler": "18.2.1",
        "geostyler-geojson-parser": "2.0.0",
        "geostyler-mapbox-parser": "6.2.0",
        "geostyler-openlayers-parser": "5.3.0",
        "geostyler-qgis-parser": "4.1.0",
        "geostyler-sld-parser": "8.2.0",
        "geostyler-style": "10.5.0",
        "geostyler-wfs-parser": "3.0.1",
        "umd-react": "19.2.1"
    }
}
