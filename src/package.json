{
    "private": false,
    "name": "@openbfs/imis3gis",
    "version": "1.18.4-SNAPSHOT",
    "description": "IMIS3 GIS",
    "scripts": {
        "pretest": "npm run lint",
        "lint": "eslint -c .eslintrc app/ classic/ modern/ overrides/ && eslint -c test/.eslintrc test/",
        "lint-fix": "eslint --fix -c .eslintrc app/ classic/ modern/ && eslint --fix -c test/.eslintrc test/",
        "test": "npm-run-all --parallel --race test:all",
        "test:all": "npm-run-all test:classic test:modern copy-coverages",
        "test:classic": "karma start karma.conf.js --single-run --toolkit classic",
        "test:modern": "karma start karma.conf.js --single-run --toolkit modern",
        "test:watch": "karma start karma.conf.js",
        "copy-coverages": "cp coverage/modern/*/*json coverage/coverage1.json && cp coverage/classic/*/*json coverage/coverage2.json",
        "combine-coverages": "nyc merge coverage/ coverage-combined/coverage.json",
        "coverage-summary": "nyc report -t coverage-combined",
        "userdoc": "gitbook pdf ../docs/userdoc ../docs/userdoc/User-Documentation.pdf",
        "jsduck": "jsduck --config jsduck-config.json",
        "clean": "rimraf apidoc build/* coverage coverage-combined"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OpenBfS/gis_client.git"
    },
    "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
    "license": "GPL-3.0",
    "devDependencies": {
        "acorn": "8.8.1",
        "braces": "3.0.2",
        "eslint": "8.31.0",
        "expect.js": "0.3.1",
        "express": "4.18.2",
        "jsduck": "1.1.2",
        "karma": "6.4.1",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage": "2.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-expect": "1.1.3",
        "karma-firefox-launcher": "2.1.2",
        "karma-mocha": "2.0.1",
        "karma-mocha-reporter": "2.2.5",
        "karma-sinon": "1.0.5",
        "lodash": "4.17.21",
        "minimatch": "5.1.2",
        "mocha": "10.2.0",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "rimraf": "3.0.2",
        "serve-static": "1.15.0",
        "sinon": "15.0.1"
    },
    "bugs": {
        "url": "https://github.com/OpenBfS/gis_client/issues"
    },
    "homepage": "https://github.com/OpenBfS/gis_client#readme",
    "main": "app.js",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "antd": "4.24.1",
        "echarts": "^5.4.1",
        "geostyler": "10.3.0",
        "geostyler-geojson-parser": "1.0.1",
        "geostyler-mapbox-parser": "3.1.1",
        "geostyler-openlayers-parser": "4.0.0",
        "geostyler-qgis-parser": "2.0.0",
        "geostyler-sld-parser": "5.0.0",
        "geostyler-style": "7.2.0",
        "geostyler-wfs-parser": "2.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    }
}
